#include <iostream>
#include <cstdio>
using namespace std;
int max_of_four(int x,int y,int z,int i)
{
    int t,t1,t2;
    t=max(x,y);
    t1=max(z,i);
    t2=max(t,t1);
    return t2;
}

int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);

    return 0;
}
